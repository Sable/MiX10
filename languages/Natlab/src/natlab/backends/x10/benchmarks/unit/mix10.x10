public class Mix10 {
public static def length(a:Array[Double]){
		return a.size;
		}
public static def minus(a:Double, b:Double){
		val x: Double;
		x = a-b;
		return x;
	    
	    }
public static def plus(a:Double, b:Double){
		val x: Double;
		x = a+b;
		return x;
	    
	    }
public static def gt(a:Double, b:Double){
		val x: Boolean;
		x = a>b;
		return x;
	    
	    }

}