function drv_foo()
% This test case shows
% Column-vector and row-vector's basic operations 
% Implicitly using vector as one dimension array

	n = floor(2.0); 

	a = ones(n, 3);
	F = ones(n, 3);
	m = ones(n, 1);

	% automatically transform vectors to one-dimensions 
	% Implicitly using m as m(:,1)
	a(:,1) = F(:, 1)./m;
		
end
